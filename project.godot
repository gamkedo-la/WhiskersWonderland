;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Whisker's Wonderland"
run/main_scene="res://title_screen.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.png"

[autoload]

Utils="*res://scripts/globals/utils.gd"
Globals="*res://scripts/globals/globals.gd"
Debug="*res://scripts/globals/debug.gd"
DampedSpringSystem="*res://scripts/damped_springs.gd"
Signals="*res://scripts/globals/signals.gd"
AudioManager="*res://scenes/audio_manager.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor]

movie_writer/movie_file="/home/adil/test.avi"

